{
  "globals": {
    "DocumentApp": false,
    "HtmlService": false
  },

  "rules": {
    "no-param-reassign": 2,
    "no-unused-vars": [2, { "vars": "local" }],

    "brace-style": [2, "stroustrup"],
    "indent": [1, 2],
    "quotes": [1, "single"],
    "wrap-iife": [1, "inside"],

    "strict": 0
  }
}
